{
  "name": "universal-react-boiler",
  "version": "0.2.0-rc.1",
  "description": "A universal React starter project.",
  "main": "build/server.js",
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/boldr-universal-react.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "universal",
    "es6",
    "es7",
    "starter",
    "javascript",
    "redux"
  ],
  "bugs": {
    "url": "https://github.com/strues/boldr-universal-react/issues"
  },
  "homepage": "https://github.com/strues/boldr-universal-react#readme",
  "scripts": {
    "build": "NODE_ENV=production babel-node tools/scripts/build.js",
    "clean": "rimraf build",
    "coverage": "codecov",
    "dev": "NODE_ENV=development PORT=3000 nodemon",
    "nuke": "rimraf node_modules/.cache && npm run clean",
    "prettier": "prettier --single-quote --print-width=100 --jsx-bracket-same-line=true --trailing-comma=all --parser=flow -w src/**/*.js",
    "start": "NODE_ENV=production node server/index.js",
    "test": "NODE_ENV=test jest",
    "test:ci": "NODE_ENV=test ./node_modules/.bin/jest"
  },
  "jest": {
    "timers": "fake",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/jest/fileMock.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/(tools|build|docs|public)/",
      "/flow-typed/",
      "/__fixtures__/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/tools/jest/transform.js"
    },
    "testRegex": ".*.test\\.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__",
      "<rootDir>/(tools|build|docs|public)/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/(tools|build|docs|public)/"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "fontfaceobserver": "^2.0.13",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "3.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-universal-component": "^2.5.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "source-map-support": "^0.5.0",
    "styled-components": "^2.2.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-self": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-universal-import": "^1.2.7",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cache-loader": "^1.1.0",
    "caniuse-lite": "1.0.30000746",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "circular-dependency-plugin": "^4.2.1",
    "codecov": "^2.3.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-boldr": "^0.13.2",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "fast-async": "^6.3.0",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "happypack": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "loader-utils": "^1.1.0",
    "lodash.merge": "^4.6.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "ora": "^1.3.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.7",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react-dev-utils": "^4.1.0",
    "react-test-renderer": "^16.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "resolve-url-loader": "^2.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "source-map-loader": "^0.2.2",
    "url-loader": "^0.6.2",
    "webpack": "3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-flush-chunks": "^1.1.23",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-hot-server-middleware": "^0.1.0",
    "webpack-subresource-integrity": "^1.0.1",
    "write-file-webpack-plugin": "^4.2.0"
  }
}
